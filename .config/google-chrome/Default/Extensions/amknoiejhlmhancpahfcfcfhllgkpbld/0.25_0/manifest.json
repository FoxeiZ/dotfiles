{
   "applications": {
      "gecko": {
         "id": "postwoman-firefox@postwoman.io"
      }
   },
   "background": {
      "persistent": false,
      "scripts": [ "index.js" ]
   },
   "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Hoppscotch Extension"
   },
   "browser_specific_settings": {
      "gecko": {
         "id": "postwoman-firefox@postwoman.io"
      }
   },
   "description": "Provides more capabilities for Hoppscotch",
   "icons": {
      "128": "icons/icon-128x128.png",
      "16": "icons/icon-16x16.png",
      "48": "icons/icon-48x48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Hoppscotch Browser Extension",
   "permissions": [ "storage", "tabs", "cookies", "\u003Call_urls>" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.25",
   "web_accessible_resources": [ "*.js.map" ]
